    "shared-element"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IjzerenHein/react-navigation-shared-element.git"
  },
  "author": "IjzerenHein <hrutjes@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IjzerenHein/react-navigation-shared-element/issues"
